<!DOCTYPE html>
<html lang="en">
<head>
  <title>Objectives for <%= day_mnemonic %></title>
  <%- include('include/_head.ejs') %>
</head>
<body>
  <%- include('include/_header.ejs') %>
  <h2><%= day_mnemonic %></h2>
<table>
  <tr>
    <th>Actions</th>
    <!-- <th>id</th> -->
    <th>type</th>
    <th>question</th>
    <th>answer</th>
    <th>sort</th>
    <!-- <th>day_id</th> -->
  </tr>
  <% for(var i=0; i < objectives.length; i++) { %>
    <tr>
      <td><form action="/delete/<%= objectives[i].id %>" method="POST"><input type="submit" value="Delete"></form>
        <form action="/edit/<%= objectives[i].id %>" method="GET"><input type="submit" value="Edit"></form></td>
      <!-- <td><%= objectives[i].id %></td> -->
      <td><%= objectives[i].type %></td>
      <td><%= objectives[i].question %></td>
      <td><%= objectives[i].answer %></td>
      <td><%= objectives[i].sort %></td>
      <!-- <td><%= objectives[i].day_id %></td> -->
    </tr>
  <% } %>
</table>

</body>
</html>
