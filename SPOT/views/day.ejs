<!DOCTYPE html>
<html lang="en">
<head>
  <title>Objectives for <%= day_mnemonic %></title>
  <%- include('include/_head.ejs') %>
</head>
<body>
  <%- include('include/_header.ejs') %>
  <h2><%= day_mnemonic %></h2>
  <h2><%= title %></h2>
  <a href="/days-edit/<%= day_id %>">edit</a>
  <a href="/new/<%= day_id %>">new</a>
  <a href="/student/<%= day_id %>">student view</a>
<table>
  <tr>
    <!-- <th>Actions</th> -->
    <!-- <th>id</th> -->
    <th>type</th>
    <th>question</th>
    <th>answer</th>
    <th>Feedback<br/>confused:fair:excellent</th>
  </tr>
  <% for(var i=0; i < objectives.length; i++) { %>
    <tr id="objective_<%= objectives[i].id %>">
      <!-- <td><%= objectives[i].id %></td> -->
      <td><%= objectives[i].type %></td>
      <td><%= objectives[i].question %></td>
      <td><%= objectives[i].answer %></td>
      <td><%= objectives[i].understandString %></td>
    </tr>
  <% } %>
</table>

<script>
  (function($) { $(document).ready(function(){
    $.ajax();
  }); })(jQuery);
</script>

</body>
</html>
